customMessage:
    elements*=Element
;

Element:
    Variable | List | Class
;


Class:
    type=ID name=ID
;

List:
    type=ID  '[' (size=INT)? ']' name=ID
;

Variable:
  type=Primitives (name=ID)?
;

Primitives:
  "bool"|"byte"|"char"|"float32"|"float64"|"int8"|"uint8"|"int16"|"uint16"|"int32"|"uint32"|"int64"|"uint64"|"string"
;

Comment:
  /\/\/.*$/
;



